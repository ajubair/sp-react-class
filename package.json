{
  "name": "sp-react-class",
  "version": "4.0.0",
  "description": "Speeding Planet's React Class",
  "main": "index.js",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "rest": "json-server --port 8001 --static src/data/ src/data/class-data-mapped.json"
  },
  "keywords": [
    "react",
    "javascript",
    "training"
  ],
  "author": "John Paxton <pax@speedingplanet.com>",
  "license": "ISC",
  "repository": "https://github.com/johnpaxton/sp-react-class",
  "devDependencies": {
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-preset-react-app": "^3.0.2",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "create-react-app": "^1.4.1",
    "css-loader": "0.28.4",
    "del": "^3.0.0",
    "dotenv": "4.0.0",
    "enzyme": "^2.8.2",
    "eslint": "4.4.1",
    "eslint-config-react-app": "^2.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-markdown": "^1.2.0",
    "gulp-wrap": "^0.13.0",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "jest-cli": "^20.0.4",
    "json-server": "^0.12.0",
    "marked": "^0.3.6",
    "minimist": "^1.2.0",
    "moment": "^2.18.1",
    "morgan": "^1.8.1",
    "opn": "^5.1.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "react-dev-utils": "^3.1.0",
    "sinon": "^3.1.0",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.7.1",
    "webpack-manifest-plugin": "1.2.1"
  },
  "dependencies": {
    "autoprefixer": "7.1.2",
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.7.0",
    "fs-extra": "3.0.1",
    "lodash": "^4.17.4",
    "normalizr": "^3.2.3",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-error-overlay": "^1.0.10",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.6.0",
    "redux-mock-store": "^1.2.3",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
